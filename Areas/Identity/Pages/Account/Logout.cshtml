@page
@model LogoutModel
@{
    ViewData["Title"] = "Log out";
}

<h2>@ViewData["Title"]</h2>

<div class="row">
    <div class="col-md-4">
        <section>
            <form method="post">
                <h4>Are you sure you want to log out?</h4>
                <button type="submit" class="btn btn-primary">Log out</button>
            </form>
        </section>
    </div>
</div>
